//#include <stack>
//#include <utility>

using namespace std;

int main()
{
	int n=0,k=0;
	scanf("%d%d", &n, &k);
	for(int i=k+1; i>1; i--)
	{
		printf("%d ", i);
	}
	printf("1");
	if(k!=n-1) printf(" ");
	for(int i=k+2; i<=n-1; i++)
	{
		printf("%d ", i);
	}
	if(k!=n-1) printf("%d", n);
	return 0;
}
